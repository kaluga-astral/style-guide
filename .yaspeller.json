{
  "excludeFiles": [
    ".git",
    ".json",
    "./**/build",
    "node_modules",
    "./**/node_modules",
    "./**/*.spec.*",
    "./**/*.test.*"
  ],
  "format": "html",
  "lang": "ru",
  "fileExtensions": [".js", ".jsx", ".ts", ".tsx", ".md", ".mdx"],
  "ignoreCapitalization": true,
  "ignoreDigits": true,
  "ignoreUrls": true,
  "maxRequests": 2,
  "ignoreText": [
    ["// .*\n", "g"],
    ["/\\*([^*]|[\\r\\n]|(\\*+([^*/]|[\\r\\n])))*\\*/", "g"]
  ],
  "dictionary": [
    "астрал",
    "валидир*",
    "токен",
    "госуслуг(и|)",
    "криптопровайдер"
  ]
}
